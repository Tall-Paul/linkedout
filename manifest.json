{
  "manifest_version": 2,

  "name": "LinkedOut",
  "description": "Block users on linkedin.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "storage",
    "contextMenus",
    "activeTab",
    "storage"
  ],
  "background": {
      "scripts": ["menu.js"]
    },
    "web_accessible_resources": ["inject.js"],
    "content_scripts": [{
      "matches": ["https://www.linkedin.com/*"],
      "js": ["jquery.js","contentscript.js"]
  }]
}
